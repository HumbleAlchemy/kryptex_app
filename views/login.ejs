				<form class="form-signin" role="form" action="/signin" method="post">
					<h2 class="form-signin-heading" style="color:white"> <%= title %></h2>
					<input type="text" class="form-control" placeholder="username" value='<%= user_name %>' style="padding:10px;margin-bottom: 10px;background: rgba(54, 25, 25, 0);color:#fff" name="user_name" required autofocus>
					<input type="password" class="form-control" placeholder="password" name="user_password"  style="padding:10px;margin-bottom: 10px;background: rgba(54, 25, 25, 0);color:#fff" required >
					<button class="btn btn-primary btn-lg btn-custom btn-block" type="submit" style="float:middle;"><span class="glyphicon glyphicon-log-in"></span> Login</button>
				</form>
			</div>
				
		</div>
